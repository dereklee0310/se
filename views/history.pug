extends layout
block head
  meta(name='generator' content='Hugo 0.104.2')
  link(rel='canonical' href='https://getbootstrap.com/docs/5.2/examples/dashboard/')
  link(href='/assets/dist/css/bootstrap.min.css' rel='stylesheet')
  meta(name='theme-color' content='#712cf9')
  // Custom styles for this template
  link(href='/css/history.css' rel='stylesheet')
block title
  title Dashboard Template &middot; Bootstrap v5.2
block content  
  .container-fluid
    .row
      nav#sidebarMenu.col-md-3.col-lg-2.d-md-block.bg-light.sidebar.collapse
        .position-sticky.pt-3.sidebar-sticky
          ul.nav.flex-column
            li.nav-item
              a.nav-link.active(aria-current='page' href='#')
                span.align-text-bottom(data-feather='home')
                |               Dashboard
            li.nav-item
              a.nav-link(href='#')
                span.align-text-bottom(data-feather='file')
                |               Orders
            li.nav-item
              a.nav-link(href='#')
                span.align-text-bottom(data-feather='shopping-cart')
                |               Products
            li.nav-item
              a.nav-link(href='#')
                span.align-text-bottom(data-feather='users')
                |               Customers
            li.nav-item
              a.nav-link(href='#')
                span.align-text-bottom(data-feather='bar-chart-2')
                |               Reports
            li.nav-item
              a.nav-link(href='#')
                span.align-text-bottom(data-feather='layers')
                |               Integrations
          h6.sidebar-heading.d-flex.justify-content-between.align-items-center.px-3.mt-4.mb-1.text-muted.text-uppercase
            span Saved reports
            a.link-secondary(href='#' aria-label='Add a new report')
              span.align-text-bottom(data-feather='plus-circle')
          ul.nav.flex-column.mb-2
            li.nav-item
              a.nav-link(href='#')
                span.align-text-bottom(data-feather='file-text')
